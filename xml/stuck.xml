<?xml version="1.0"?>
<!-- Test file: Flocker Project, Split -->
<!-- A flock will split to avoid obstacles and then regroup to pursue a light
      but fail because they can not plan how to go around such big obstacles.
       THEIR PERSISTENCE/INTELLIGENCE IS GOOD ENOUGH TO CATCH THE LIGHT IF
         THE OBSTACLES ARE ANY SMALLER THAN THIS!!!-->

<world xmlns="http://perceptualscience.rutgers.edu/flocking"
   width="750" height="600" time="60" debug="true">
<defaults>
   <!-- default settings -->
   <flocker 
         debug="false" clear="true" viewAngle="180"
         evade="true" align="true" center="true" follow="true"/>
</defaults>
<flocker x="50" y="250" heading="0" size="25"
         r="255" g="0" b="0"/>
<flocker x="100" y="250" heading="0" size="25"
         r="0" g="0" b="255"/>
<flocker x="75" y="175" heading="0" size="25"
         r="0" g="0" b="0"/>
<flocker x="25" y="250" heading="0" size="25"
         r="185" g="185" b="185"/>
<flocker x="25" y="200" heading="0" size="25"
         r="255" g="125" b="100"/>
<flocker x="100" y="330" heading="0" size="25"
         r="0" g="255" b="0"/>
<rock    x="300" y="250" size="216" ow="10" />
<light x="450" y="250" r="100" g="0" b="0" />
<rock    x="600" y="250" size="216" ow="10" />
<!-- <rock    x="400" y="185" size="45" ow="10" />
<rock    x="500" y="115" size="45" ow="10" /> -->
</world>

