<?xml version="1.0"?>
<!-- Test file: Flocker Project, Split -->
<!-- A flocker will split to avoid an obstacle and then regroup to pursue a light -->

<world xmlns="http://perceptualscience.rutgers.edu/flocking"
   width="750" height="600" time="120" debug="true" >
<defaults>
   <!-- default settings -->
   <flocker 
         debug="false" clear="true" viewAngle="180"
         evade="true" align="true" center="true" follow="true" />
</defaults>
<flocker x="125" y="250" heading="0" size="25"
         r="255" g="0" b="0"/>
<flocker x="125" y="150" heading="0" size="25"
         r="0" g="0" b="255"/>
<flocker x="150" y="250" heading="0" size="25"
         r="0" g="0" b="0"/>
<flocker x="150" y="150" heading="0" size="25"
         r="185" g="185" b="185"/>
<flocker x="200" y="225" heading="0" size="25"
         r="255" g="125" b="100"/>
<flocker x="200" y="175" heading="0" size="25"
         r="0" g="255" b="0"/>
<rock    x="350" y="95" size="25" ow="10" />
<rock    x="250" y="175" size="25" ow="10" />
<light x="420" y="90" r="100" g="0" b="0" />
</world>

